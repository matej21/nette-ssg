{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"lib": [
			"esnext",
			"dom"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitOverride": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext"
	}
}
